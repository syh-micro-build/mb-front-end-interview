{
  "extends": ["@commitlint/config-conventional"],
  "rules": {
    "type-enum": [
      2,
      "always",
      [
        "js",
        "ts",
        "vue",
        "style",
        "refactor",
        "react",
        "vue"
      ]
    ],
    "subject-case": [2, "never", ["start-case", "pascal-case"]]
  }
}
